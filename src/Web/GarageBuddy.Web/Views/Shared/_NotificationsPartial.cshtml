@using static GarageBuddy.Common.Constants.NotificationConstants

<script src="~/themes/mazer/dist/assets/extensions/sweetalert2/sweetalert2.min.js"></script>

@if (TempData.ContainsKey(NotifyError))
{
    <script>toastr.error('@TempData[NotifyError]');</script>
}

@if (TempData.ContainsKey(NotifyWarning))
{
    <script>toastr.warning('@TempData[NotifyWarning]');</script>
}

@if (TempData.ContainsKey(NotifyInfo))
{
    <script>toastr.info('@TempData[NotifyInfo]');</script>
}

@if (TempData.ContainsKey(NotifySuccess))
{
    <script>toastr.success('@TempData[NotifySuccess]');</script>
}